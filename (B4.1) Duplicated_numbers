def get_numbers():
    numbers = []
    for i in range(10):
        num = int(input(f"Enter number {i+1}: "))
        numbers.append(num)
    return numbers

def find_duplicates(numbers):
    duplicates = []
    for num in numbers:
        if numbers.count(num) > 1 and num not in duplicates:
            duplicates.append(num)
    return duplicates
