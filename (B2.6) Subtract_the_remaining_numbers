# Ask the user to input 10 numbers
numbers = []    # List to store the numbers

for i in range(10):
   value = float(input(f"Enter number {i + 1}: "))
   numbers.append(value)

# Calculate the result
result = numbers[0]    # Start with the first number
for value in numbers[1:]:    # Subtract the remaining numbers
   result -= value

# Print the result
print(f"The result of {numbers[0]} minus all the remaining numbers is:", result)
