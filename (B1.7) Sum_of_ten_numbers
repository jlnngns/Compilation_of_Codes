def sum_of_ten_numbers():
    total_sum = 0    # Initialize sum

    for i in range(10):
      entered_value = int(input(f"Enter number {i + 1}: "))    # Get user input
      total_sum += entered_value   # Add number to total

    print("The sum of all numbers is:", total_sum)   # Print sum

sum_of_ten_numbers()
