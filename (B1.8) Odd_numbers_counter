# Initialize a counter for odd numbers
odd_count = 0

# Ask the user to input ten numbers
for i in range(10):
  user_input = int(input(f"Enter number {i + 1}: "))   # Convert input to integer
  if user_input % 2 != 0:   # Check if the number is odd
       odd_count += 1

# Print the count of odd numbers
print("The number of odd numbers is:", odd_count)
